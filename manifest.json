{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"PS2 Dashboard",
		"version":"2.1.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Cesar CRamsan Ramirez",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Planetside 2 Dashboard with lots of utilities for players. It includes, stats, killboard, crosshairs, teamspeak integration and more..."
	},
	"permissions": ["GameControl",],
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"clickthrough":false,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":false,
					"size":{"width":450, "height":230},
					"min_size":{"width":420, "height":90},
					"max_size":{"width":512, "height":258}
			},
			"Killboard":{
					"file":"Files/killboard.html",
					"transparent":true,
					"resizable":true,
					"clickthrough":false,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":true,
					"size":{"width":550, "height":300},
					"min_size":{"width":550, "height":65},
					"max_size":{"width":670, "height":355}
			},
			"Outfit":{
					"file":"Files/outfit.html",
					"transparent":true,
					"resizable":true,
					"clickthrough":false,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":true,
					"size":{"width":550, "height":300},
					"min_size":{"width":334, "height":65},
					"max_size":{"width":670, "height":550}
			},
			"Stats":{
					"file":"Files/stats.html",
					"transparent":true,
					"resizable":true,
					"clickthrough":false,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":true,
					"min_size":{"width":450, "height":87},
					"max_size":{"width":600, "height":286},
					"size":{"width":500, "height":175},
			},
			"ReticleDot":{
					"file":"Files/reticle_dot.html",
					"transparent":true,
					"resizable":false,
					"clickthrough":true,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":true,
					"size":{"width":220, "height":220},
			},
			"ReticleCircle":{
					"file":"Files/reticle_circle.html",
					"transparent":true,
					"resizable":false,
					"clickthrough":true,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":true,
					"size":{"width":220, "height":220},
			},
			"ReticleCross":{
					"file":"Files/reticle_cross.html",
					"transparent":true,
					"resizable":false,
					"clickthrough":true,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":true,
					"size":{"width":220, "height":220},
			},
			"ChatCommands":{
					"file":"Files/chatcommands.html",
					"transparent":true,
					"resizable":true,
					"clickthrough":false,
					"grab_keyboard_focus":false,
					"ignore_keyboard_events":false,
					"min_size":{"width":290, "height":100},
					"max_size":{"width":290, "height":350},
					"size":{"width":290, "height":350},
			},
		}
	}
}
